<ivysettings>
  <settings defaultResolver="chain"/>
  <resolvers>
    <chain name="chain" dual="true">

      <ibiblio name="maven" 
               root="http://repo2.maven.org/maven2/"
               m2compatible="true"
							 usepoms="false" />

      <ibiblio name="cloudera" 
               root="https://repository.cloudera.com/content/repositories/public/" 
               m2compatible="true"
							 usepoms="false" />
    </chain>
  </resolvers>
</ivysettings>
