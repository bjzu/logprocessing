<ivysettings>
  <settings defaultResolver="cloudera"/>
  <resolvers>
    <chain name="chain" dual="true">
      <ibiblio name="cloudera" 
               root="https://repository.cloudera.com/content/repositories/public/" 
               m2compatible="true" />

      <ibiblio name="ibiblio"
               m2compatible="true" />
    </chain>
  </resolvers>
  <modules>
    <module organisation="com.cloudera" name="*" resolver="chain" />
  </modules>
</ivysettings>

